{
  "name": "vicops-terminal",
  "version": "2.4.0-open-beta",
  "description": "Terminal for vicops. Used vicops-api",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-7": "pkg index.js -o build/vicops-term-win7.exe --target node12-win32",
    "build-lin": "pkg index.js -o build/vicops-term-linux --target linux",
    "build-mac": "pkg index.js -o build/vicops-term-mac --target macos",
    "build": "pkg index.js -o build/vicops-term.exe --target win32",
    "build-all": "npm run build && npm run build-7 && npm run build-lin && npm run build-mac"
  },
  "keywords": [],
  "author": "artegoser",
  "license": "MIT",
  "dependencies": {
    "cfonts": "^2.10.0",
    "colors": "^1.4.0",
    "jsonwebtoken": "^9.0.0",
    "node-localstorage": "^2.2.1",
    "prompts": "^2.4.2",
    "table": "^6.8.1",
    "vicops-api": "^2.3.0"
  }
}
